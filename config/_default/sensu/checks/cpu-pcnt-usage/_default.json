{
  "gem": "sensu-plugins-cpu-checks",
  "attributes": {
    "standalone": true,
    "type": "metric",
    "output_type": "graphite",
    "auto_tag_host": true,
    "command": "metrics-cpu-pcnt-usage.rb",
    "interval": 10,
    "subscribers": [
      "default"
    ],
    "handlers": [
      "relay",
      "elasticsearch_metrics"
    ]
  }
}
