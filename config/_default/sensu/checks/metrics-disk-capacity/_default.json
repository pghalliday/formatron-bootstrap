{
  "gem": "sensu-plugins-disk-checks",
  "attributes": {
    "standalone": true,
    "type": "metric",
    "output_type": "graphite",
    "auto_tag_host": true,
    "command": "metrics-disk-capacity.rb",
    "interval": 10,
    "subscribers": [
      "default"
    ],
    "handlers": [
      "relay"
    ]
  }
}
